{
  "npmScope": "anti-food-waste",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/nx-cloud",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"],
        "accessToken": "OTIxN2FhYWEtNjdjOC00YzQ2LTk1MGItNDk2M2RkN2ZiMzQyfHJlYWQtd3JpdGU="
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "cms": {
      "tags": []
    },
    "cms-components": {
      "tags": []
    },
    "cms-e2e": {
      "tags": [],
      "implicitDependencies": ["cms"]
    },
    "cms-pages": {
      "tags": []
    },
    "cms-parts": {
      "tags": []
    },
    "mobileapp": {
      "tags": []
    },
    "mobileapp-components": {
      "tags": []
    },
    "mobileapp-data-access": {
      "tags": []
    },
    "mobileapp-e2e": {
      "tags": [],
      "implicitDependencies": ["mobileapp"]
    },
    "mobileapp-pages": {
      "tags": []
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "@emotion/styled",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "@emotion/styled"
      },
      "library": {
        "style": "@emotion/styled",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "cms"
}
