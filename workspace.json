{
  "version": 2,
  "projects": {
    "cms": {
      "root": "apps/cms",
      "sourceRoot": "apps/cms/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/cms",
            "index": "apps/cms/src/index.html",
            "main": "apps/cms/src/main.tsx",
            "polyfills": "apps/cms/src/polyfills.ts",
            "tsConfig": "apps/cms/tsconfig.app.json",
            "assets": [
              "apps/cms/src/favicon.ico",
              "apps/cms/src/assets"
            ],
            "styles": [],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/cms/src/environments/environment.ts",
                  "with": "apps/cms/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "cms:build",
            "hmr": true
          },
          "configurations": {
            "production": {
              "buildTarget": "cms:build:production",
              "hmr": false
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "apps/cms/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/cms"
          ],
          "options": {
            "jestConfig": "apps/cms/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "cms-components": {
      "root": "libs/cms/components",
      "sourceRoot": "libs/cms/components/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "libs/cms/components/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/cms/components"
          ],
          "options": {
            "jestConfig": "libs/cms/components/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "cms-e2e": {
      "root": "apps/cms-e2e",
      "sourceRoot": "apps/cms-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/cms-e2e/cypress.json",
            "devServerTarget": "cms:serve",
            "tsConfig": "apps/cms-e2e/tsconfig.json"
          },
          "configurations": {
            "production": {
              "devServerTarget": "cms:serve:production"
            }
          }
        },
        "lint": {
